<div class="catalog_container">
	<div class="page-header dog_page-header" align="center">
		<div class="header_container">
	  		<h1><small class="catalog_header">{{breed}}</small></h1>
	  	</div>
	</div>
	<div class="container">
		<div class="row dog_row">
			<div class="col-xs-12 col-md-2">
				<div class="panel panel-info dog_panel-info">
					<div class="panel-heading">
						<h3 class="panel-title dog_panel-title">Sub-breeds</h3>
					</div>
					<div class="panel-body">
						<div class="row">
						{{#each list.message as |item|}}
							{{#if @first}}
								<div class="col-xs-12 col-md-12">
									<div class="btn-group-vertical" role="group">
										<button type="button" class="btn btn-default dog_btn-default">{{item}}</button>
							{{else if @last}}
										<button type="button" class="btn btn-default dog_btn-default">{{item}}</button>
									</div>
								</div>
							{{else}}
										<button type="button" class="btn btn-default dog_btn-default">{{item}}</button>
							{{/if}}
						{{/each}}
						</div>
					</div>
				</div>
			</div>

	    </div>
	</div>
</div>
